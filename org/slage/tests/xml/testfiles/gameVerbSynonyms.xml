<?xml version='1.0' standalone='no'?>
<game xmlns="http://org.slage.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://org.slage.xml ../../../../xml/slageXMLElementSchema.xsd"
      name="test game">


 
     
     <handler type="org.slage.handlers.ResponseHandler">
             <verb value="look" synonyms="true" />
             <verb value="contemplate" synonyms="true" />
             
             <string name="text" value="foo" />
             <string name="sound" value="foo" />
     </handler>
     
     
     <handler type="org.slage.handlers.ResponseHandler">
             <verb value="look" synonyms="false" />
             
             <string name="text" value="foo2" />
             <string name="sound" value="foo2" />
     </handler>
     
     
     <handler type="org.slage.handlers.ResponseHandler">
             <verb value="look" >
                <omit value="ogle" />
                <omit value="scan" />
                <omit value="peep" />
             </verb>
             
             <string name="text" value="foo3" />
             <string name="sound" value="foo3" />
     </handler>
     
         <verbSynonyms verb="look" action="create" >
             <synonym value="examine" />
             <synonym value="observe" />
             <synonym value="peep" />
             <synonym value="check" />
             <synonym value="glance" />
             <synonym value="scan" />
             <synonym value="skim" />
             <synonym value="peruse" />
             <synonym value="ogle" />
             <synonym value="gaze" />
             <synonym value="eyeball" />
          </verbSynonyms>
     
        </game>
        
     
     
        


