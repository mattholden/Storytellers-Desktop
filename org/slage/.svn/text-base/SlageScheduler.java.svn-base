package org.slage;
import org.slage.command.Commander;
import org.slage.framework.scheduler.Scheduler;

/**
 * Extend the Scheduler to be a Commander 
 * @author Jaeden
 */
public class SlageScheduler extends Scheduler implements Commander {
    
    /** The game to schedule for */
    private SlageGame game;
    
    /** Creates a new instance of SlageScheduler 
        @param theGame the game to schedule for */
    public SlageScheduler(SlageGame theGame ) 
    {
        game = theGame;        
    }
    
    /** Getter for game.
     * @return Value of property game.      */
    public SlageGame getGame() { return game;     }        
    
}
